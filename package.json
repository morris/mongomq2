{
  "name": "mongomq2",
  "version": "0.1.0",
  "description": "A general-purpose message and event queuing library for MongoDB",
  "keywords": [
    "mongodb",
    "message",
    "messaging",
    "queue",
    "event",
    "stream"
  ],
  "homepage": "https://github.com/morris/mongomq2",
  "repository": {
    "type": "git",
    "url": "https://github.com/morris/mongomq2.git"
  },
  "bugs": {
    "url": "https://github.com/morris/mongomq2/issues"
  },
  "contributors": [
    "Morris Brodersen <mb@morrisbrodersen.de> (https://morrisbrodersen.de)"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write './**/*.ts' './**/*.js' './**/*.json'",
    "format-check": "prettier --check './**/*.ts' './**/*.js' './**/*.json'",
    "lint": "eslint .",
    "test": "jest --runInBand --collectCoverage",
    "build": "tsc",
    "docs": "typedoc src/index.ts --excludeProtected --excludePrivate",
    "maybe-publish": "bash scripts/maybe-publish.sh"
  },
  "peerDependencies": {
    "mongodb": "^4"
  },
  "dependencies": {
    "mingo": "^6.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^14.18.10",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "jest": "^27.5.0",
    "mongodb": "^4.3.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}
